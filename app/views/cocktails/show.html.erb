<h1>Here are all the cocktails!</h1>
<p>Find me in app/views/cocktails/show.html.erb</p>

<h2><%= @cocktail.name %></h2>

<ul>
<% @cocktail.doses.each do |dose| %>
  <li><%= dose.description %> of <%= dose.ingredient.name %> <%= link_to 'delete this' , dose_path(dose), method: 'delete' %></li>
<% end %>
</ul>
